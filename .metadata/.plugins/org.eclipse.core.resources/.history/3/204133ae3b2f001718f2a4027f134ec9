#include "GameEngine.h"

int main() {

	vector<string> data { "##########", "####.....#", "###......#",
			"##.......#", "#........#", "#........#", "#........#",
			"#........#", "#........#", "##########", };
	for(int i = 0; i < data.size(); i++){
		cout << data.at(i) << "\n";
	}
	GameEngine ge(10, 10, data);
	ge.run();
	return 0;

}
