#ifndef TITLE_H_
#define TITLE_H_

#include "Character.h"


typedef enum {
	Floor,
	Wall
} Typ;

class Title{
private:
	Typ m_typ;
	Character* m_character = nullptr;

public:
	Title(Typ typ, Character* character);
	Typ getTyp();
	Character* getCharacter();
	bool hasCharacter();
	void setCharacter(Character* character);
};



#endif /* TITLE_H_ */
