#include "Ticket.h"

Ticket::Ticket() {
	m_text = "NULL";
	m_owner = "NOONE";
	m_status = duplicate;
	m_id = id_counter;

	id_counter++;
}

Ticket::Ticket(string ownerName, string description) {
	m_text = description;
	m_owner = ownerName;
	m_status = open;
	m_id = id_counter;

	id_counter++;
}

string Ticket::getText() const{
	return m_text;
}

string Ticket::getOwner() const{
	return m_owner;
}

int Ticket::getId() const{
	return m_id;
}

Status Ticket::getStauts() const{
	return m_status;
}
