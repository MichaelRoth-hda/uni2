#ifndef SRC_PUNKT_H_
#define SRC_PUNKT_H_

#include <iostream>
#include "Punkt.cpp"

using namespace std;
class Punkt{
private:
	int m_x;
	int m_y;
	bool m_blocked;
public:
	Punkt();
	Punkt(int x, int y);
	Punkt(int x, int y, bool newState);
	bool toogleState();
	bool getState();
	int getX();
	int getY();
};


#endif /* SRC_PUNKT_H_ */
