#include "Tile.h"

Title::Title(Typ typ, Character* character) {
	m_typ = typ;
	m_character = character;
}

Typ Title::getTyp() {
	return m_typ;
}

Character* Title::getCharacter() {
	return m_character;
}

bool Title::hasCharacter(){
	if(m_character == nullptr)
		return false;
	return true;
}

void Title::setCharacter(Character* character){
	m_character = character;
}
